<packages>
    
    <package name='BDSKY' version='1.2.1'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.2.0/BDSKY.addon.1.2.1.zip"
           description="birth death skyline - handles serially sampled tips, piecewise constant rate changes through time and sampled ancestors">
        <depends on='beast2' atleast='2.2.0'/>
    </package>

    <package name='BEASTlabs' version='1.2.2'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.2.0/BEASTlabs.addon.v1.2.2.zip"
           description="BEAST utilities, such as multi threaded treeliklihood, Script">
        <depends on='beast2' atleast='2.2.0'/>
    </package>

    <package name='BEAST_CLASSIC' version='1.1.8'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.2.0/BEAST_CLASSIC.addon.v1.1.8.zip"
           description="BEAST classes ported from BEAST 1 in wrappers" >
        <depends on='beast2' atleast='2.2.0'/>
        <depends on='BEASTlabs' atleast='1.2.1'/>
    </package>

    <package name='GEO_SPHERE' version='0.0.5'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.2.0/GEO_SPHERE.addon.v0.0.5.zip"
           description="Whole world phylogeography" >
        <depends on='beast2' atleast='2.2.0'/>
    </package>

    <package name='CA' version='1.0.4'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.2.0/CA.addon.v1.0.4.zip"
           description="CladeAge aPackage for fossil calibrations">
        <depends on='beast2' atleast='2.2.0'/>
    </package>

    <package name='MASTER' version='4.0.0'
