<packages>
    <!--names in alphabetic order-->

    <package name='BDSKY' version='1.1.1'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.1.2/BDSKY.addon.v1.1.1.zip"
           description="birth death skyline -- handles serially sampled tips">
        <depends on='beast2' atleast='2.1.0'/>
    </package>

    <package name='BEASTlabs' version='1.1.2'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.1.2/BEASTlabs.addon.v1.1.2.zip"
           description="BEAST utilities, such as multi threaded treeliklihood, Script">
        <depends on='beast2' atleast='2.1.2'/>
    </package>

    <package name='BEAST_CLASSIC' version='1.1.3'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.1.2/BEAST_CLASSIC.addon.v1.1.3.zip"
           description="BEAST classes ported from BEAST 1 in wrappers" >
        <depends on='beast2' atleast='2.1.2'/>
        <depends on='BEASTlabs' atleast='1.1.2'/>
    </package>

    <package name='CA' version='1.0.0'
           url="http://beastii.googlecode.com/files/CA.addon.v1.0.0.zip"
           description="CladeAge aPackage for fossil calibrations">
        <depends on='beast2' atleast='2.1.0'/>
    </package>

    <package name='DISSECT' version='1.0.0'
           url="http://www.indriid.com/2014/DISSECT.addon.v1.0.0.zip"
           description="species delimitation with *BEAST">
        <depends on='beast2' atleast='2.1.0'/>
    </package>

    <package name='MASTER' version='1.10.0'
           url="https://github.com/CompEvol/MASTER/releases/download/v1.10.0/MASTER.package.v1.10.0.zip"
           description="stochastic simulation" >
        <depends on='beast2' atleast='2.1.0'/>
    </package>
    
    <package name='MultiTypeTree' version='3.0.0'
           url="https://github.com/CompEvol/MultiTypeTree/releases/download/v3.0.0/MultiTypeTree.v3.0.0.zip"
           description="structured coalescent simulation" >
        <depends on='beast2' atleast='2.1.0'/>
    </package>
    
    <package name="MODEL_SELECTION" version="1.0.1."
            url="https://github.com/CompEvol/CBAN/releases/download/v2.1.2/MODEL_SELECTION.addon.v1.0.1.zip"
            description="Select models through path sampling/stepping stone analysis">
        <depends on='beast2' atleast='2.1.2'/>
    </package>

    <package name='phylodynamics' version='1.0.1'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.1.2/Phylodynamics.addon.v1.0.1.zip"
           description="birth death skyline model" >
        <depends on='beast2' atleast='2.1.0'/>
        <depends on='BDSKY' atleast='1.1.1'/>
    </package>

    <package name='RBS' version='1.1.0'
           url="http://rb-beast.googlecode.com/files/RBS.addon.v1.1.0.zip"
           description="Reversible-jump Based substitution model" >
        <depends on='beast2' atleast='2.1.0'/>
    </package>

    <package name='SA' version='1.0.0'
           url="http://beastii.googlecode.com/files/SA.addon.v1.0.0.zip"
           description="Sampled ancestor trees" >
        <depends on='beast2' atleast='2.1.0'/>
    </package>

    <package name='SNAPP' version='1.1.5'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.1.2/SNAPP.addon.v1.1.5.zip"
           description="SNP and AFLP Phylogenies" >
        <depends on='beast2' atleast='2.1.0'/>
    </package>

    <package name='SubstBMA' version='1.1.0'
           url="https://github.com/CompEvol/CBAN/releases/download/v2.1.2/SubstBMA.addon.v1.1.0.zip"
           description="Substitution Bayesian Model Averaging" >
        <depends on='beast2' atleast='2.1.0'/>
    </package>

</packages>
